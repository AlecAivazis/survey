{
    "problemMatcher": [
        {
            "owner": "go",
            "pattern": [
                {
                    "regexp": "^\\s*(.+\\.go):(\\d+):(?:(\\d+):)? (.*)",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4
                }
            ]
        }
    ]
}
